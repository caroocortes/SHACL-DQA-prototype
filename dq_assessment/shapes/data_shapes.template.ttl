{% macro consistency_deprecated_classes(classes_list, type_property) -%}
ex:DeprecatedClassesShape
    a sh:NodeShape ;
    sh:targetSubjectsOf <{{type_property}}> ;
    sh:or (
        [
            sh:path rdf:type ;
            sh:hasValue rdfs:Class ;
        ]
        [
            sh:path rdf:type ;
            sh:hasValue rdf:Property ;
        ]
